{
  "Serilog": {
    "Enrich": [
      "WithCorrelationIdHeader"
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {CorrelationId} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "InvoiceContext": "SQL Server Connection String"
  },
  "Servicebus": {
    "PrimaryConnectionString": "Service Bus Connection String"
  },
  "Likvido": {
    //"BaseAddress": "https://api.likvido.com/api/v1/", //PROD
    "BaseAddress": "https://localhost:44310/api/",
    "ApiKey": "Likvido API key",
    "SendInvoiceApiPath": "Invoices"
  },
  "ProcessPendingInvoiceCorn": "Quartz CORN expression"
}
